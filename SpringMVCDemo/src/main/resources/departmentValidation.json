{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Department",
    "description": "A department from Acme's catalog",
    "type": "object",
    "properties": {
        "departmentId": {
            "description": "The unique identifier for a product",
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "deleteFlag": {
            "type": "boolean"
        },
        "employeeList":{
        	"type": "array"
        }
    },
    "required": ["name", "deleteFlag"]
}